{
  "name": "LunarWolf",
  "license": "MIT",
  "version": "1.0.1",
  "sideEffects": false,
  "description": "Extensible, fast and innovative web browser with material UI.",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/IroniumStudios/LunarWolf-Browser/blob/main/README.md",
  "main": "build/main.bundle.js",
  "author": "Damon Smith <cleverdamontoutube@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IroniumStudios/LunarWolf-Browser.git"
  },
  "discussion": {
    "url": "https://github.com/IroniumStudios/LunarWolf-Browser/issues"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "nuke": "rimraf node_modules",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "compile-pacman": "npm run build && electron-builder --linux pacman",
    "deploy": "npm run build && electron-builder build --win --linux appimage snap --publish never",
    "lint": "concurrently \"eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"\" \"prettier --check \"src/**/*.ts*\" \"src/**/*.tsx*\"\"",
    "lint-fix": "concurrently \"eslint \"src/**/*.ts*\" \"src/**/*.tsx*\" --fix\" \"prettier --write \"src/**/*.ts*\" \"src/**/*.tsx*\"\"",
    "rebuild": "electron-builder install-app-deps",
    "copyright": "copyright-header --templateId minimal --forceModificationYear 2024 --fix --copyrightHolder Damon Smith --include src"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@electron/remote": "^2.1.2",
    "@ironiumstudios/rpc-core": "^3.0.1",
    "@ironiumstudios/rpc-electron": "^3.0.1",
    "@ironiumstudios/rpc-node": "^3.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@seald-io/nedb": "^4.0.4",
    "@types/animejs": "^3.1.12",
    "@types/chrome": "^0.0.268",
    "@types/crypto-js": "^4.2.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.15.0",
    "animejs": "^3.2.2",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-to-clipboard": "^3.3.3",
