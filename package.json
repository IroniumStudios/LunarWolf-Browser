{
  "name": "lunarwolf",
  "version": "2.2.0",
  "sideEffects": false,
  "description": "LunarWolf",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "homepage": "https://github.com/LunarWolf-Browser-Projects/lunarwolf-browser/blob/main/README.md",
  "main": "build/main.bundle.js",
  "original-author": "Eryk Rakowski <sentialx@gmail.com>",
  "author": "Damon Smith <cleverdamontoutube@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LunarWolf-Browser-Projects/LunarWolf-Browser.git"
  },
  "discussion": {
    "url": "https://github.com/LunarWolf-Browser-Projects/LunarWolf-Browser/issues"
  },
  "resolutions": {
    "mobx": "6.3.3",
    "mobx-react-lite": "3.4.0"
  },
  "scripts": {
    "dev-renderer": "cross-env DEV=1 webpack serve --config webpack.config.renderer.js",
    "dev-webpack": "cross-env DEV=1 webpack",
    "build-renderer": "webpack --config webpack.config.renderer.js",
    "dev": "cross-env START=1 npm run watch",
    "build": "rimraf build && concurrently \"npm run build-renderer\" \"webpack\"",
    "nuke": "rimraf node_modules",
    "ci-build": "node scripts/ci-build.js",
    "start": "electron .",
    "watch": "concurrently \"npm run dev-renderer\" \"npm run dev-webpack\"",
    "compile-win32": "npm run build && electron-builder -w",
    "compile-darwin": "npm run build && electron-builder -m",
    "compile-linux": "npm run build && electron-builder -l",
    "compile-pacman": "npm run build && electron-builder --linux pacman",
    "deploy": "npm run build && electron-builder build --win --linux appimage snap --publish never",
    "lint": "concurrently \"eslint \"src/**/*.ts*\" \"src/**/*.tsx*\"\" \"prettier --check \"src/**/*.ts*\" \"src/**/*.tsx*\"\"",
    "lint-fix": "concurrently \"eslint \"src/**/*.ts*\" \"src/**/*.tsx*\" --fix\" \"prettier --write \"src/**/*.ts*\" \"src/**/*.tsx*\"\"",
    "rebuild": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@electron/remote": "^2.1.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@seald-io/nedb": "^4.0.4",
    "@types/animejs": "^3.1.12",
    "@types/chrome": "^0.0.287",
    "@types/crypto-js": "^4.2.2",
    "@types/jszip": "^3.4.1",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "animejs": "^3.2.2",
    "awesome-node-loader": "^1.1.1",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.0",
    "copy-to-clipboard": "^3.3.3",
    "copy-webpack-plugin": "^12.0.2",
    "copyright-header": "^0.4.6",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0-beta.7",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-updater": "^6.3.9",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "file-loader": "6.2.0",
    "file-type": "18.2.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "gitmoji-changelog": "^2.3.0",
    "html-webpack-plugin": "^5.6.3",
    "icojs": "^0.19.4",
    "jszip": "^3.10.1",
    "mobx": "6.3.3",
    "mobx-react-lite": "3.4.0",
    "node-fetch": "^3.3.2",
    "node-parse-bookmarks": "^1.0.3",
    "prettier": "^3.4.1",
    "pretty-bytes": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-refresh": "^0.14.2",
    "react-windows-controls": "^1.1.1",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "styled-components": "^6.1.13",
    "terser": "^5.36.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-plugin-styled-components": "^3.0.0",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
