    "@wexond/rpc-electron": "^1.0.3",
    "@wexond/rpc-node": "^1.0.3",
    "electron-chrome-extensions-suit": "^1.0.8",
    "electron-chrome-web-store": "^0.1.0",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "react-query": "^3.39.3",
