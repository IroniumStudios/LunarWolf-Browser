    "@fortawesome/react-fontawesome": "^0.2.2",
    "electron-chrome-extensions-suit": "^1.0.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "react-query": "^3.39.3",
    "react-use": "^17.5.0",
